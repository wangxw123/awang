<listener>
	<listener-class>
	
	com.reapal.wxw.utils.ServletRequestAttribute
	</listener-class>

</listener>
  
 <listener>
 	<listener-class>
 		com.reapal.wxw.utils.ServletRequestLis
 	</listener-class>
 
 </listener>






